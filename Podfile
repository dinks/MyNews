target 'MyNews' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MyNews
  pod 'Alamofire'
  pod 'AlamofireImage'

  target 'MyNewsTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MyNewsUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
